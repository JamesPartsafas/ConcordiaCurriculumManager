{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "[[FROM CLI]]",
  "IdentitySettings": {
    "Key": "[[FROM CI]]",
    "Issuer": "[[FROM CLI]]",
    "SecurityAlgorithms": "[[FROM CLI]]",
    "Audience": "[[FROM CLI]]"
  },
  "DatabaseSettings": {
    "ConnectionString": "[[FROM CLI]]"
  },
  "CorsSettings": {
    "AllowedWebsite": "[[FROM CLI]]"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "PostgreSQL",
        "Args": {
          "connectionString": "[[FROM CLI]]",
          "tableName": "ProdLogs",
          "schemaName": null,
          "needAutoCreateTable": true,
          "loggerColumnOptions": {
            "Id": "IdAutoIncrement",
            "TimeStamp": "Timestamp",
            "LevelAsText": "LevelAsText",
            "Message": "Message",
            "Exception": "Exception",
            "Properties": "Properties"
          },
          "period": "0.00:00:30",
          "batchSizeLimit": 50
        }
      }
    ]
  }
}