$(document).ready(function() {
    var isSubsite = ($("input#is_subsite").val() === "true");

    // Check for employee personae and set action to private search
    var personae = getCookie("Personae");
    if (personae != null
        && personae.includes("employee")
        && !personae.includes("inactive-employee")
        && $("input#private_url").val() != undefined
        && $("input#private_url").val() != ""
    ) {
        $("#site-search").attr("action", $("input#private_url").val());
        $("#concordia_url").val($("input#private_url").val());
    }

    // Put focus into search input when search icon is clicked
    $(".concordia-search-icon.d-none.d-lg-block").on("click", function() {
        $(this).toggleClass("open");
        $("#input-search").focus();
    });

    $("#site-search").submit(function() {
        var searchMode = $("input[name=search-mode]:checked").val();
        var searchTerm = $("input#input-search").val();

        if (searchMode == "concordia-search") {
            $("#input-search").attr("name", "q");
            return true;
        } else if (searchMode == "cspace-search") {
            $("#input-search").attr("name", "query");
            return true;
        } else if (searchMode == "directory-search") {
            if ($("#site_search_first_name").val() != "" && $("#site_search_last_name").val() != "") {
                window.location.href = $("input#directory_url").val() + "?f=search&fname="+ $("#site_search_first_name").val() + "&lname=" + $("#site_search_last_name").val();
            } else {
                window.location.href = $("input#directory_url").val() + "?f=search&name="+ searchTerm;
            }
        } else if (searchMode == "encore-search") {
            // Custom for Library
            sofiaSearch();
        } else if (searchMode == "library-search") {
            return true;
        }

        return false;
    });

    // Radio button interactions
    $("input[name=search-mode]").change(function() {
        var placeholder = "Search " + $(this).parent().text().trim();
        var searchType = $(this).val();

        // Change the action of the form based on selection
        if (searchType == "concordia-search") {
            $("#site-search").attr("action", $("input#concordia_url").val());
        } else if (searchType == "directory-search") {
            $("#site-search").attr("action", $("input#directory_url").val());
        } else if (searchType == "cspace-search") {
            $("#site-search").attr("action", $("input#cspace_url").val());
        }

        // Custom for Library
        else if (searchType == "encore-search") {
            $("#site-search").attr("action", $("input#encore_url").val());
            placeholder = $(this).parent().text().trim();
        } else if (searchType == "library-search") {
            placeholder += " website";
            $("#site-search").attr("action", $("input#library_url").val());
        }

        $("#input-search").attr("placeholder", placeholder);
        $("#input-search").autocomplete("option", "source", search_terms_json_uri + "?type=" + which_search_terms_source());
    })
    .on("click", function() {
        $("#input-search").focus();
    });

    // Auto complete
    var search_terms_json_uri = "/etc/designs/concordia/resources/search-terms.json"
    function which_search_terms_source() {
        if ($("input[name=search-mode]:checked").val() == "concordia-search") {
            return "terms";
        } else if ($("input[name=search-mode]:checked").val() == "directory-search") {
            return "directory";
        }
    };

    // Check if ID exists
    if (document.getElementById("input-search") != null) {
        $("#input-search")
            .autocomplete({
                minLength: 3,
                source: search_terms_json_uri + "?type=" + which_search_terms_source(),
                focus: function( event, ui ) {
                    $("#input-search").val(ui.item.label);
                    return false;
                },
                select: function( event, ui ) {
                    $("#input-search").val(ui.item.label);
                    return false;
                },
                response: function(event, ui) {
                    if (ui.content.length == 1){
                        $("#site_search_first_name").val(ui.content[0].firstname);
                        $("#site_search_last_name").val(ui.content[0].lastname);
                    } else {
                        $("#site_search_first_name").val("");
                        $("#site_search_last_name").val("");
                    }
                },
            })
            .data("ui-autocomplete")
            ._renderItem = function(ul, item) {
                var html_output;
                if (item.type == "terms") {
                    html_output = "<a>" + item.label + "<br><span class=\"autolink\" onclick=\"location.href='" + item.link + "' ;return false;\">Visit this website</span></a>";
                } else if (item.type == "directory") {
                    var employee_directory_url = $("input#directory_url").val()
                    var employeeLink = employee_directory_url + "?sort=name&fname=" + item.firstname + "&lname=" + item.lastname;
                    html_output = "<a><span class=\"autolink\" onclick=\"location.href='" + employeeLink + "' ; return false;\">" + item.label + "</span>, Phone: " + item.phone + "</a>";
                }
                return $("<li></li>")
                    .data("item.autocomplete", item)
                    .append(html_output)
                    .appendTo(ul);
            };
    }

    // Sticky navigation and search when scrolling
    // Only execute these behaviors on desktop
    if ($(".concordia-logo-top.d-lg-none").css("display") == "none") {
        var nav = $("header #navigation-wrapper");
        var search = $("header .site-search");
        var autocomplete = $(".ui-autocomplete");
        var navOffset = nav.offset().top;
        var finalOffset = navOffset;

        // Calculate the top value from top position of search + its height when open
        // Note: no easy way to determine height with javascript, because it's 0 by default, so it's hardcoded here
        var autocompleteOffset = search.offset().top + 53;
        var autocompleteFinalOffset = autocompleteOffset;

        $(window).scroll(function() {
            // Check if quick links is open
            var quicklinksHeight = $(".c-quick-links").height();
            if (quicklinksHeight > 0) {
                finalOffset = navOffset + quicklinksHeight;
                autocompleteFinalOffset = autocompleteOffset + quicklinksHeight;
            } else {
                finalOffset = navOffset;
                autocompleteFinalOffset = autocompleteOffset;
            }

            if ($(this).scrollTop() >= finalOffset) {
                // Hide search when entering sticky mode
                if (!$(nav).hasClass("sticky") && $("#site-search").height() > 0) {
                    $("#site-search").collapse("hide");
                    $("a.concordia-search-icon").removeClass("open");
                    $(".ui-autocomplete").css("display", "none");
                }

                $(nav).addClass("sticky");
                $(search).addClass("sticky");
                $(autocomplete).addClass("sticky");
                $(autocomplete).css("top", autocompleteFinalOffset);
            } else {
                $(nav).removeClass("sticky");
                $(search).removeClass("sticky");
                $(autocomplete).removeClass("sticky");
            }
        });

        // Close search when user clicks away
        $("html").on("click", function(event) {
            var target = $(event.target);

            // Make sure that click is not inside search, the close search icon
            // Also, keep search open if there are 3 or more entered characters
            if (!target.closest("#site-search").length && !target.hasClass("concordia-search-icon") && $("#input-search").val().length < 3) {
                if ($("#site-search").height() > 0) {
                    $("#site-search").collapse("hide");
                    $("a.concordia-search-icon").removeClass("open");
                }
            }
        });
    }
});

