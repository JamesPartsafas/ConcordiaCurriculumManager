function toggleAlertText($item, seeLessLabel, seeMoreLabel) {
    $($item).find(".btn-see-more").on("click", function(e) {
        var thisInstance = $(this).closest($item);
        var isPreview = thisInstance.find(".btn-see-more").hasClass("open") ? false : true;

        if (isPreview) {
            thisInstance.find(".description").html(thisInstance.find(".description-full").html());
            $(this).find(".js-expand-btn-label").text(seeLessLabel);
            thisInstance.find(".btn-see-more").addClass("open");
            if ($(".btn-see-more").hasClass("open")) {
                thisInstance.find(".description").attr("tabindex", "-1");
                thisInstance.find(".description").focus();
                toggleArrow($item);
            }

        } else {
            thisInstance.find(".description").html(thisInstance.find(".description-preview").html());
            $(this).find(".js-expand-btn-label").text(seeMoreLabel);
            thisInstance.find(".btn-see-more").removeClass("open");

            if (!$(".btn-see-more").hasClass("open")) {
                toggleArrow($item);
            }
        }

        e.preventDefault();
        return false;
    });
}

function toggleArrow($item) {
    $($item).find(".ci-chevron-up").toggleClass("rotate-180");
}

