// General scripts for context logic (header, navigation, and footer)
$(document).ready(function() {
    // Get the 'context' from the current path retrieved from og:url meta tag which includes full unrewritten URL.
    var path = "";
    try {
        var pageUrl = new URL(document.querySelector('meta[property="og:url"]').content); // Parse value as a URL
        path = pageUrl.pathname; // Extract the pathname portion - what comes after the domain and port.
    } catch (_){
        if (DEBUG) console.log("Path invalid");
    }
    var cqs = ""; // Initialize C.ontext Q.uery S.tring variable
    /**
     * We only wish to set a context if the current path does not include the substring:
     * /content/shared
     * or start with any of these tree/directory locations:
     * /content/hub/en/carrefour/news
     * /content/hub/en/carrefour/events
     * /content/concordia/fr/actualites
     * /content/concordia/en/news
     * /content/concordia/en/events
     * 
     * These are default contexts and so do not require explicit specification.
     **/
    if (!path.includes("/content/shared")
        && !path.startsWith("/content/hub/en/carrefour/news")
        && !path.startsWith("/content/hub/en/carrefour/events")
        && !path.startsWith("/content/concordia/fr/actualites")
        && !path.startsWith("/content/concordia/en/news")
        && !path.startsWith("/content/concordia/en/events")
    ) {
        cqs = path.replace("/content/concordia/en", "")
            .replace("/content/concordia/fr", "")
            .replace("/content/hub/en", "")
            .replace("/content/hub/fr", "")
            .replace(".html", "");
        if (cqs != ""){  // Prefix ?c= to string if it is not empty
            cqs = "?c=" + cqs;
        }
    }
     // If the path is on the hub, add Hub site_identifier query string parameter
    if (path.includes("/content/hub")) {
        cqs += (cqs.includes("?") ? "&" : "?" ) + "site_identifier=hub";
    }
    // If page already has a context query string (/content/shared page), use that directly.
    var contextParams = new URLSearchParams(window.location.search);
    if (contextParams.has("c") || contextParams.has("site_identifier")) {
        cqs = window.location.search;
    }

    // Listen for clicks on hyperlinks in shared content list components, then add CQS and browse to destination.
    $(".c-news-list .row a")
        .add(".c-news-list .text a")
        .add(".list-events .accordion a")
        .add(".events-list .accordion a")
        .add(".events-list .event a")
        .add(".events-list .row a")
        .add(".events-list .text a")
        .add(".c-events-list a")
        .add(".c-list-featured-events .event a")
        .add(".c-relatedstories a")
        .add(".social-media a")
        .add(".c-eventbar .row a")
        .add(".carousel-item a")
        .add(".c-faculty-list a")
        .on("click", function(e) {
        var href = $(this).attr("href");
        if (cqs != "" && 
            (href.includes("/content/shared")
            || href.includes("cunews")
            || href.includes("/news/")
            || href.includes("cuevents")
            || href.includes("/faculty/"))) {
            if (href.includes("?")) {
                cqs = cqs.replace("?","&");
            }
            e.preventDefault();
            window.location.href = href + cqs;
        }
    });
});
