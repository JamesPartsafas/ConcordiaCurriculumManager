// General javascript that applies to multiple components or templates
$(document).ready(function() {
    // For browsers that do not support Javascript's basic startsWith function, this adds it globally
    if (!String.prototype.startsWith) {
        String.prototype.startsWith = function(searchString, position) {
            position = position || 0;
            return this.indexOf(searchString, position) === position;
        };
    }

    // When the font size is modified in the WYSIWYG or Text/Image components, it's applied to the span level
    // Make sure the numbers in ordered lists also have the same font size
    $("ol li:has(span.xlarge-text)").addClass("xlarge-text");
    $("ol li:has(span.large-text)").addClass("large-text");

    $("a").each(function() {
        targetBlankLinkAccessibility(this);
    });
});

// Links that open in a new tab should have an "aria-label" attribute
// explaining to screen-readers that this opens in a new tab
function targetBlankLinkAccessibility(link) {
    if (link.href !== undefined) {
        var external = true;
        if (link.href.includes(".concordia.ca")
            || link.href.includes("/content/concordia/")
            || link.href.includes("/content/hub/")
            || link.href.includes("/content/shared/")
            || link.href.includes("/content/dam/")
        ) {
            external = false;
        }

        if ($(link).attr("target") == "_blank") {
            var message = $(link).text().trim();
            if (message == "") {
                message = ($(link).attr("title") !== "undefined") ? $(link).attr("title") : "";
            }
            if (message !== undefined && message != "") {
                message += (external) ? " (external link opens in a new tab)" : " (link opens in a new tab)";
            } else {
                message = (external) ? "External link opens in a new tab" : "Link opens in a new tab";
            }
            $(link).attr("aria-label", message);
        }
    }
}

// Get query string values
function getParameterByName(name, url) {
    if (!url) {
        url = window.location.href;
    }
    name = name.replace(/[\[\]]/g, "\\$&");
    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)");

    var results = regex.exec(url);
    if (!results) {
        return null;
    }
    if (!results[2]) {
        return "";
    }
    return decodeURIComponent(results[2].replace(/\+/g, " "));
}

function scrollToAnchor(anchor, useAnimation) {
    var anchorTag = $(anchor);
    var scrollSpeed = 0;

    if (anchorTag != undefined) {
        // Offset from top to adjust for the sticky nav
        var offset = ($(window).width() < 992) ? 12 : 80;
        if (useAnimation != false) {
            scrollSpeed = "fast";
        }
        $("html, body").animate({scrollTop: anchorTag.offset().top - offset}, scrollSpeed);

    }
}

// Stop animation when user scrolls
$(window).on("mousewheel", function() {
    $("html, body").stop();
});

