function toastInit(instanceId, isEdit) {
    const instanceEl = $("#" + instanceId).parent();
    const buttonCloseEl = instanceEl.find(".btn-close");

    // Determine if the Toast should be shown by checking the cookie
    let toastCookie = Cookies.get("concordia-toast");
    var showToastCheck = true;

    // Always show the Toast in Edit Mode by ignoring the cookie
    if (isEdit) {
        toastCookie = null;
    }

    if (toastCookie !== undefined && toastCookie !== null && toastCookie != "" && toastCookie != "undefined") {
        // Parse the JSON string into an array of objects
        var cookieObjects = JSON.parse(toastCookie);

        // Iterate through each item in the array of objects
        cookieObjects.forEach(function(item) {
            // Don't show the Toast if the current instanceId is in the cookie and it hasn't expired
            if (item.instanceId == instanceId) {
                let currentDate = new Date();
                let expirationDate = new Date(item.expires);
                if (currentDate <= expirationDate) {
                    showToastCheck = false;
                }
            }
        });
    }

    // Show the Toast if it hasn't been closed by the user (check cookie) and it's not over the footer
    if (showToastCheck && !toastOverFooter()) {
        showToast(instanceEl);
    }

    // Add a click event listener to the close button
    buttonCloseEl.on("click", function () {
        instanceEl.css("display", "none");

        // Set expiration in 24 hours from now
        let expirationDate = new Date();
        expirationDate.setTime(expirationDate.getTime() + (24 * 60 * 60 * 1000));
        let expires = expirationDate.toUTCString();

        // Create a new array of objects and add the current instanceId to it
        let newCookieObjects = [{"instanceId": instanceId, "expires": expires}];

        // Parse the array of objects already set in the cookie from previous instances
        if (cookieObjects !== undefined && cookieObjects !== null) {
            let currentDate = new Date();

            // Iterate through each item in the array of objects
            cookieObjects.forEach(function(item) {
                // Skip the current instanceId because it was already added above
                if (item.instanceId != instanceId) {
                    let expirationDate = new Date(item.expires);

                    // Only add the objects to the new array that haven't expired
                    // This removes cookie items that have expired to keep the cookie size smaller
                    if (currentDate <= expirationDate) {
                        newCookieObjects.push(item);
                    }
                }
            });
        }

        // Convert the array of objects to a JSON string
        let cookieString = JSON.stringify(newCookieObjects);

        // Set a session cookie
        Cookies.set("concordia-toast", cookieString);
    });

    // When the user scrolls, hide the Toast when it's over the footer
    window.addEventListener("scroll", function () {
        if (toastOverFooter()) {
            instanceEl.css("opacity", 0);
        } else {
            // Reset opacity when scrolling up above the footer
            instanceEl.css({
                "opacity": "1",
                "transform": "translate(0, 0)"
            });
        }
    });
}


// Check if Toast is over the footer
function toastOverFooter() {
    const footerEl = document.querySelector(".c-footer");
    const footerTopEl = document.querySelector(".custom-footer-top");
    let footerPosition = (footerTopEl) ? footerTopEl.getBoundingClientRect().top : footerEl.getBoundingClientRect().top;
    var footerOffset = footerPosition - window.innerHeight;

    if (footerOffset <= 10) {
        return true
    } else {
        return false;
    }
}


// Show the Toast using animation (on page load the Toast is hidden)
function showToast(instanceEl) {
    // Animate the Toast on page load
    setTimeout(function () {
        instanceEl.css({
            "opacity": "1",
            "transform": "translate(0, 0)"
        });
    }, 300);
}

// Convert Value Prop Block into an accordion on smaller screens
function windowResizedValuePropBlock(item) {
    var button = item.find(".c-value-prop-block__title");
    if ($(window).width() < 768) {
        button.attr("aria-expanded", "false");
        button.attr("aria-pressed", "false");
        button.attr("data-bs-toggle", "collapse");
        button.attr("role", "button");
        button.attr("tabindex", 0);

        $(button).keypress(function(e) {
            if (e.which === 13) {
                item.find(".collapse").collapse("toggle");
            }
        });
    } else {
        button.attr("aria-expanded", "true");
        button.attr("data-bs-toggle", "");
        button.attr("role", "");
        button.attr("tabindex", -1);
    }
}

