var DEBUG = false;

function manageFieldsEventsList(field){
    //Get the panel of the tab our drop down menu on
    var panel = field.findParentByType("panel");

    // Our text fields are stored in widgets of type dialogfieldset and we get their reference
    var fieldSets = panel.findByType("dialogfieldset");
    var fLength = fieldSets.length;

    // Get value of selected option in out select box
    var fieldValue = field.getValue();
    if (DEBUG) {
        console.log("Field Value: " + fieldValue);
    }

    for (var i = 0; i < fLength ; i++) {
        var fieldSet = fieldSets[i];

        // Custom logic
        if (fieldSet.getItemId() == "featured" && (fieldValue == "featured" || fieldValue == "vignettes")) {
            fieldSet.show();
        } else if (fieldSet.getItemId() == "accordion" && fieldValue == "accordion") {
            fieldSet.show();
        } else if (fieldSet.getItemId() == "dateRange" && fieldValue == "date-range") {
            fieldSet.show();
        } else if (fieldSet.getItemId() == "importantDates" && fieldValue == "default_important_dates") {
            fieldSet.show();
        } else if (fieldSet.getItemId() != "tags") {
            fieldSet.hide();
        }
    }
}

$(window).on("load", function() {
    // "Back to top" icon behaviour
    if ($("#c-back-to-top").length) {
        var documentHeight = $(document).height();
        var windowHeight = window.innerHeight;
        var windowWidth = window.innerWidth;
        //var footerHeight = 0;
        //var footerOffset = 0;
        var mobile = false;
        //var responsivePreviousState = $(".mobile-footer").is(":visible") ? "mobile" : "desktop";
        //var responsiveToggle = false; // Flag when browser toggles between the responsive break points
        checkIfMobile();

        function checkIfMobile() {
            mobile = true;
            if ($(".mobile-footer").is(":visible")) {
                //footerOffset = $(".mobile-footer").offset().top;
                //footerHeight = $(".mobile-footer").height();
                //responsiveToggle = (responsivePreviousState != "mobile") ? true : false;
                //responsivePreviousState = "mobile";
            } else {
                mobile = false;
                //footerOffset = $(".c-footer").offset().top;
                //footerHeight = $(".c-footer").height();
                //responsiveToggle = (responsivePreviousState != "desktop") ? true : false;
                //responsivePreviousState = "desktop";
            }
        }

        function adjustIconPosition(source) {
            //console.log("adjustIconPosition(): " + source);
            checkIfMobile();
            var yTop = $(window).scrollTop();
            //var yBottom = yTop + windowHeight;
            //var iconOffset = $("#c-back-to-top").offset().top;

            // "Back to top" icon appears after user scrolls 2 screens down
            if (yTop >= windowHeight * 2) {
                if (!$("#c-back-to-top").is(":visible")) {
                    $("#c-back-to-top").fadeIn(300);
                }

                /*
                // January 26, 2022: do not stick the icon to the footer because it relies
                // on too much JS to adjust its position. There is no way to rely only on CSS
                // for the position of the icon because the current layout can have a
                // horizontal bar between 769 and 1200 pixels. So, "position: absolute" and
                // "position: fixed" don't work the same way. Once the overall layout is
                // updated such that it never has a horizontal bar, we can re-add the feature
                // of the icon sticking to the top of the footer.

                // Make the icon stick halfway at the top of the footer
                // 22px is the midline of the icon (44px is its height)
                if (yBottom - 44 >= footerOffset || yTop == iconOffset || responsiveToggle) {
                    $("#c-back-to-top").css("bottom", (yBottom - footerOffset - 22) + "px");
                }

                // Unstick the icon from the footer and use fixed position when scrolling up
                // 20px is the bottom margin
                else {
                    $("#c-back-to-top").css("bottom", "20px");
                }
                */
            } else {
                $("#c-back-to-top").fadeOut(300);
            }
        }

        $(window).on("resize", function() {
            windowHeight = window.innerHeight;
            windowWidth = window.innerWidth;
            checkIfMobile();
            adjustIconPosition("resize");

            let leftMargin = (mobile) ? "20" : "30";
            // 44px is the height of the icon
            $("#c-back-to-top").css("left", windowWidth - 44 - leftMargin + "px");
        });

        $(document).scroll(function() {
            adjustIconPosition("scroll");
        });

        // "Back to top" animation
        $("#c-back-to-top").on("click", function(e) {
            $("body,html").animate({scrollTop: 0}, 600);
            e.preventDefault();
        });
    }
});

